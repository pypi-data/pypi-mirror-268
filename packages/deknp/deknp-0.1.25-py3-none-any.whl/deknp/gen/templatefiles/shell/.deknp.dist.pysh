deknp build

@pyo var_pn [x for x in mp.os.listdir("./server/.material") if x.endswith(".yaml")][0]
@pyo var_pn mp.os.path.splitext("{var_pn}")[0]

dektools file write ./server/{var_pn}/main/static/main/frontend --c=./dist

@cc rp ./.deknp.dist
dektools file merge ./.deknp.dist/backend ./server/{var_pn} --ignore=.gitignore
dektools file merge ./.deknp.dist/frontend ./dist
